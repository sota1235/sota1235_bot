{
  "name": "sota1235_bot",
  "private": true,
  "main": "index.js",
  "engines": {
    "node": "16.x"
  },
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "@sentry/node": "^6.13.2",
    "@sentry/tracing": "^6.13.2",
    "@slack/bolt": "^3.7.0",
    "@slack/web-api": "^6.0.0",
    "@types/express-serve-static-core": "^4.17.21",
    "@types/ioredis": "^4.17.0",
    "@types/jest": "^29.0.0",
    "@types/jsdom": "^20.0.0",
    "@types/lodash": "^4.14.182",
    "@types/luxon": "^2.0.0",
    "@types/node-cron": "^3.0.0",
    "@types/node-fetch": "^2.5.12",
    "dotenv": "^16.0.0",
    "iconv-lite": "^0.6.2",
    "ioredis": "^4.17.3",
    "jsdom": "^20.0.0",
    "luxon": "^2.0.0",
    "node-cron": "^3.0.0",
    "node-fetch": "^2.6.5",
    "source-map-support": "^0.5.20",
    "typescript": "^4.0.0"
  },
  "devDependencies": {
    "@sota1235/eslint-config": "^3.0.0",
    "eslint": "^8.0.0",
    "husky": "8.0.1",
    "jest": "29.1.2",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "swagger-typescript-api": "^9.3.1",
    "ts-jest": "29.0.3",
    "ts-node": "^10.0.0"
  },
  "scripts": {
    "build": "npm run build:ts",
    "build:ts": "tsc",
    "watch": "tsc -w",
    "start:dev": "node -r dotenv/config lib/src/index.js",
    "start:prod": "node lib/src/index.js",
    "fix": "npm run prettier && npm run eslint:fix",
    "lint": "npm run _prettier -- -c && npm run eslint",
    "eslint": "eslint 'src/**/*.ts'",
    "eslint:fix": "npm run eslint --fix",
    "generate:remo:client": "ts-node tool/generateRemoClient.ts",
    "_prettier": "prettier './src/**/*.ts'",
    "prettier": "npm run _prettier -- --write",
    "debug": "ts-node -r dotenv/config",
    "test": "jest",
    "test:ci": "jest -c ./jest.config.ci.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "npm run fix"
    ]
  }
}
